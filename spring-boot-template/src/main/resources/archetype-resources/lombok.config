# Tell the lombok configuration system not to search for config files
#in the parent directories
config.stopBubbling = true

#Add @lombok.Generated annotation to all the code generated by Lombok,
# so it can be ignore by annotation processors & test coverage reports
lombok.addLombokGeneratedAnnotation = true

# Generate constructors with parameters annotated by the Springâ€™s @Value annotation
#copied from fields
lombok.copyableannotations += org.springframework.beans.factory.annotation.Value